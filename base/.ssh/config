Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly yes
  TCPKeepAlive yes

Host *
  Protocol 2
  ControlPath none
  CheckHostIP no
  HashKnownHosts yes
  Compression yes
  SendEnv LANG LC_* TZ TERM COLORTERM
  ForwardAgent no
  ForwardX11 no
  ForwardX11Trusted yes
  ServerAliveInterval 60
  ServerAliveCountMax 3
